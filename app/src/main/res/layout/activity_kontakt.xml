<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Kontakt">

    <ImageView
        android:id="@+id/kontaktBillede"
        android:layout_width="70dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        app:srcCompat="@drawable/greve"
        tools:layout_editor_absoluteX="315dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/kontakttekst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="23dp"
        android:text="Kontakt Greve Tennisklub"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/KontaktSlogan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="13dp"
        android:text="Den mest fortrukne klub syd og vest for København" />

    <TextView
        android:id="@+id/emne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/kontakttekst"
        android:layout_marginTop="172dp"
        android:text="Emne:" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="245dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/kontakttekst"
        android:layout_marginTop="199dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/besked"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/kontakttekst"
        android:layout_marginBottom="222dp"
        android:text="Besked:" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="301dp"
        android:layout_height="67dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/kontakttekst"
        android:layout_marginBottom="102dp"
        android:ems="10"
        android:inputType="textMultiLine"
        android:singleLine="false" />

    <Button
        android:id="@+id/button_send"
        android:layout_width="77dp"
        android:layout_height="38dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="40dp"
        android:layout_marginEnd="25dp"
        android:text="Send" />

    <EditText
        android:id="@+id/til"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/kontakttekst"
        android:layout_below="@+id/navn"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Kavehv@live.dk"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="265dp"
        android:layout_height="53dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/kontakttekst"
        android:layout_marginTop="70dp"
        android:text="Har du et spørgsmål? Stil det nedenfor. Vi vil forsøge at besvare dit spørgsmål hurtigst muligt." />

</RelativeLayout>